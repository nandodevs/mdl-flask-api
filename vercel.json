{
    "version": 2,
    "builds": [
      {
        "src": "app.py", 
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/doramas-stars",
        "methods": ["GET"],
        "dest": "app.py"
      },
      {
        "src": "/api/series/dublado",
        "methods": ["GET"],
        "dest": "app.py"
      },
      {
        "src": "/api/series/legendado",
        "methods": ["GET"],
        "dest": "app.py"
      }
    ]
  }
  