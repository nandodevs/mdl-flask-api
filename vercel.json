{
    "version": 2,
    "builds": [
      {
        "src": "api/doramas-stars.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/doramas-series.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/doramas-stars", "dest": "api/doramas-stars.py" },
      { "src": "/doramas-series", "dest": "api/doramas-series.py" }
    ]
  }
  